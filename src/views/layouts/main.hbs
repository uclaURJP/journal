---
root: ..
---

<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <title>
            UCLA Undergraduate Psychology Research Journal
        </title>

        <link rel="shortcut icon" href="{{ root }}/img/favicon.ico">
        <link rel="apple-touch-icon-precomposed" href="{{ root }}/img/touch-icon.png">

        <link rel="stylesheet" type="text/css" href='http://fonts.googleapis.com/css?family=Lato:100,300'>
        <link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
        <link rel="stylesheet" type="text/css" href="{{ root }}/css/style.min.css">
    </head>

    <body>
        <div id="container">
            <header>
                <a href="{{ root }}">
                    <div id="logo">
                    </div>
                </a>

                <nav>
                    <ul>
                        <li><a href="{{ root }}/" {{#is active_tab 'home'}}class="active"{{/is}}>Home</a></li>
                        <li><a href="{{ root }}/submissions" {{#is active_tab 'submissions'}}class="active"{{/is}}>Submissions</a></li>
                        <li><a href="{{ root }}/members" {{#is active_tab 'members'}}class="active"{{/is}}>Members</a></li>
                        <li><a href="{{ root }}/publications" {{#is active_tab 'publications'}}class="active"{{/is}}>Publications</a></li>
                        <li><a href="{{ root }}/contact" {{#is active_tab 'contact'}}class="active"{{/is}}>Contact Us</a></li>
                        <li><a href="{{ root }}/links" {{#is active_tab 'links'}}class="active"{{/is}}>Resources</a></li>
                        <li><a href="{{ root }}/about" {{#is active_tab 'about'}}class="active"{{/is}}>About Us</a></li>
                        <li><a href="{{ root }}/alumni" {{#is active_tab 'alumni'}}class="active"{{/is}}>Alumni</a></li>
                        <li><a href="{{ root }}/workshops" {{#is active_tab 'workshops'}}class="active"{{/is}}>Workshops</a></li>
                    </ul>
                </nav>
            </header>

            <div id="main">
                <h1 class="page-header">{{ page_header }}</h1>
                {{> body }}
            </div>

            <div id="push">
                <!--Sticky footer hackery-->
            </div>
        </div>

        <footer>
            <div>&copy; 2015 Undergraduate Research Journal of Psychology at UCLA</div>
            <div>Designed and Created by <a href="http://ryanhansberry.com">Ryan Hansberry</a>
        </footer>

        <script src="{{ root }}/js/main.min.js"></script>
    </body>
</html>


